  "Resources": {
    "PrivateSubnet": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "CidrBlock": "192.168.0.0/24",
        "MapPublicIpOnLaunch": "true",
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-private-subnet"
          },
          {
            "Key": "Application",
            "Value": {
              "Ref": "AWS::StackId"
            }
          }
        ],
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "bfc9a21f-8ebb-4e29-999f-1457ce042db4"
        }
      }
    },
    "InternetGateway": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-ig"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "ddcd6644-7b3b-45b3-ab84-f8917a9748d5"
        }
      }
    },
    "VPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "192.168.0.0/24",
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-vpc"
          },
          {
            "Key": "Application",
            "Value": {
              "Ref": "AWS::StackId"
            }
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "3ec442c6-29fd-45ad-8ff2-ecb2e3dce920"
        }
      }
    },
    "ControllerInstance": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "InstanceType": {
          "Ref": "ControllerInstanceType"
        },
        "IamInstanceProfile": {
          "Ref": "RestrictedProfile"
        },
        "ImageId": {
          "Ref": "ControllerAMI"
        },
        "KeyName": {
          "Ref": "KeyName"
        },
        "NetworkInterfaces": [
          {
            "NetworkInterfaceId": {
              "Ref": "nic0"
            },
            "DeviceIndex": "0"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-controller"
          },
          {
            "Key": "StackHost",
            "Value": "Controller"
          },
          {
            "Key": "automation_group",
            "Value": "{{ devstack_name }}_controller"
          }
        ],
        "UserData": {
          "Fn::Base64": {
            "Fn::Join": [
              "",
              [
                "#!/bin/bash", "\n",
{% for cmd in bootstrap %}
                {{ cmd | to_json }}, "\n",
{% endfor %}
                ""
              ]
            ]
          }
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "4b80e302-c4a6-42ad-92e2-90b08b10f870"
        }
      }
    },
{% for node in nodes.computeNodes[:devstack_compute_nodes] %}
    "ComputeInstance{{ node.suffix }}": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "InstanceType": {
          "Ref": "ComputeInstanceType"
        },
        "IamInstanceProfile": {
          "Ref": "RestrictedProfile"
        },
        "ImageId": {
          "Ref": "ComputeAMI"
        },
        "KeyName": {
          "Ref": "KeyName"
        },
        "NetworkInterfaces": [
          {
            "NetworkInterfaceId": {
              "Ref": "{{ node.nic }}"
            },
            "DeviceIndex": "0"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-compute{{ node.suffix }}"
          },
          {
            "Key": "StackHost",
            "Value": "Compute{{ node.suffix }}"
          },
          {
            "Key": "automation_group",
            "Value": "{{ devstack_name }}_compute"
          }
        ],
        "UserData": {
          "Fn::Base64": {
            "Fn::Join": [
              "",
              [
                "#!/bin/bash", "\n",
{% for cmd in bootstrap %}
                {{ cmd | to_json }}, "\n",
{% endfor %}
                ""
              ]
            ]
          }
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "{{ node.uuid }}"
        }
      }
    },
{% endfor %}
    "CommonIncomingRules": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "Allow ICMP, SSH (Port 22)",
        "SecurityGroupIngress": [
          {
            "IpProtocol": "-1",
            "CidrIp": "192.168.0.0/16"
          },
          {
            "IpProtocol": "-1",
            "CidrIp": "10.0.0.0/8"
          },
          {
            "IpProtocol": "icmp",
            "FromPort": "-1",
            "ToPort": "-1",
            "CidrIp": "0.0.0.0/0"
          },
          {
            "CidrIp": "0.0.0.0/0",
            "FromPort": "22",
            "ToPort": "22",
            "IpProtocol": "tcp"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-sg-common"
          }
        ],
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "cb82b184-7d27-4c07-a4c4-e783e9d40b7a"
        }
      }
    },
    "ControllerIncomingRules": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "Allow HTTP (Port 80)",
        "SecurityGroupIngress": [
	  {
            "CidrIp": "0.0.0.0/0",
            "FromPort": 80,
            "ToPort": 80,
            "IpProtocol": "tcp"
          },
          {
            "CidrIp": "0.0.0.0/0",
            "FromPort": "6080",
            "ToPort": "6080",
            "IpProtocol": "tcp"
          }
	],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-sg-http"
          }
        ],
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "c01c1fcd-e244-4dbc-b1c2-acf41086e8d6"
        }
      }
    },
    "NetworkACL": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-nw-acl"
          }
        ],
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "3c0773b6-cd1c-47ea-a135-478a25df05c9"
        }
      }
    },
    "InboundACL": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "RuleNumber": "100",
        "Protocol": "-1",
        "RuleAction": "allow",
        "Egress": "false",
        "CidrBlock": "0.0.0.0/0",
        "NetworkAclId": {
          "Ref": "NetworkACL"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "c1300c28-3a4f-4ca5-ad07-a1ffffc0ce81"
        }
      }
    },
    "OutboundACL": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "RuleNumber": "100",
        "Protocol": "-1",
        "RuleAction": "allow",
        "Egress": "true",
        "CidrBlock": "0.0.0.0/0",
        "NetworkAclId": {
          "Ref": "NetworkACL"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "02f9240c-87e2-480b-9f0c-8bdc7eaccff7"
        }
      }
    },
    "PrivateSubnetACL": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "NetworkACL"
        },
        "SubnetId": {
          "Ref": "PrivateSubnet"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "59aceedb-8981-414f-a932-7c39e5338dca"
        }
      }
    },
    "nic0": {
      "Type": "AWS::EC2::NetworkInterface",
      "Properties": {
        "PrivateIpAddress": "{{ nodes.controllerNode.ip }}",
        "SourceDestCheck": "false",
        "SubnetId": {
          "Ref": "PrivateSubnet"
        },
        "GroupSet": [
          {
            "Ref": "ControllerIncomingRules"
          },
          {
            "Ref": "CommonIncomingRules"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-nic0"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "823ba251-91bf-41b4-9743-800ce02c81af"
        }
      }
    },
{% for node in nodes.computeNodes[:devstack_compute_nodes] %}
    "{{ node.nic }}": {
      "Type": "AWS::EC2::NetworkInterface",
      "Properties": {
        "PrivateIpAddress": "{{ node.ip }}",
        "SourceDestCheck": "false",
        "SubnetId": {
          "Ref": "PrivateSubnet"
        },
        "GroupSet": [
          {
            "Ref": "CommonIncomingRules"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-{{ node.nic }}"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "{{ node.nic_uuid }}"
        }
      }
    },
{% endfor %}
    "InternetRoute": {
      "DependsOn": "InternetGatewayAttachment",
      "Type": "AWS::EC2::Route",
      "Properties": {
        "DestinationCidrBlock": "0.0.0.0/0",
        "GatewayId": {
          "Ref": "InternetGateway"
        },
        "RouteTableId": {
          "Ref": "RoutingTable"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "df6e7900-b672-45d7-8dfa-03497768c984"
        }
      }
    },
    "SUBRTE517KA": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "RouteTableId": {
          "Ref": "RoutingTable"
        },
        "SubnetId": {
          "Ref": "PrivateSubnet"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "c76d6884-4dab-4366-a022-5fe577ccbb72"
        }
      }
    },
    "InternetGatewayAttachment": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "InternetGatewayId": {
          "Ref": "InternetGateway"
        },
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "9386d08e-cfac-480d-a64a-409b23217427"
        }
      }
    },
    "RoutingTable": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": "{{ devstack_name }}-routes"
          }
        ],
        "VpcId": {
          "Ref": "VPC"
        }
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "f8d30cbe-fcc4-43e3-bf90-dc3c1972c1a9"
        }
      }
    },
    "LimitedRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "ec2.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "Path": "/"
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "44344a2e-25e2-41ae-9bf8-5f3b0b5ea347"
        }
      }
    },
    "RolePolicy": {
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "limited_access_to_s3_buckets",
        "PolicyDocument": {
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "s3:ListBucket"
              ],
              "Resource": [
                "arn:aws:s3:::romana-binaries"
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "s3:DeleteObject",
                "s3:GetObject",
                "s3:PutObject"
              ],
              "Resource": [
                "arn:aws:s3:::romana-binaries/*"
              ]
            },
            {
              "Action": [
                "route53:ChangeResourceRecordSets",
                "route53:GetHostedZone",
                "route53:ListResourceRecordSets"
              ],
              "Effect": "Allow",
              "Resource": [
                "arn:aws:route53:::hostedzone/Z3CT83X5Z3DBU6"
              ]
            }
          ]
        },
        "Roles": [
          {
            "Ref": "LimitedRole"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "6831e819-e5cd-410d-9d00-06873a3b0032"
        }
      }
    },
    "RestrictedProfile": {
      "Type": "AWS::IAM::InstanceProfile",
      "Properties": {
        "Path": "/",
        "Roles": [
          {
            "Ref": "LimitedRole"
          }
        ]
      },
      "Metadata": {
        "AWS::CloudFormation::Designer": {
          "id": "d1a2f6de-482a-43e9-8572-2dc7617605c7"
        }
      }
    }
  },
  "Parameters": {
    "KeyName": {
      "Description": "Name of an existing EC2 KeyPair to enable SSH access to the instance",
      "Type": "AWS::EC2::KeyPair::KeyName",
      "Default": "shared-key-1",
      "ConstraintDescription": "Must be the name of an existing EC2 KeyPair."
    },
    "ControllerInstanceType": {
      "Description": "EC2 instance type for Devstack Controller Node",
      "Type": "String",
      "Default": "t2.micro",
      "ConstraintDescription": "Must be a valid EC2 instance type."
    },
    "ControllerAMI": {
      "Description": "AMI to use on Controller Node",
      "Type": "String",
      "Default": "ami-df6a8b9b",
      "ConstraintDescription": "Ubuntu 14.04"
    },
    "ComputeInstanceType": {
      "Description": "EC2 instance type for Devstack Compute Nodes",
      "Type": "String",
      "Default": "t2.micro",
      "ConstraintDescription": "Must be a valid EC2 instance type."
    },
    "ComputeAMI": {
      "Description": "AMI to use on Compute Nodes",
      "Type": "String",
      "Default": "ami-df6a8b9b",
      "ConstraintDescription": "Ubuntu 14.04"
    },
    "DevstackBranch": {
      "Description": "Devstack Version to install",
      "Type": "String",
      "Default": "master"
    },
    "RomanaBranch": {
      "Description": "Romana version to install",
      "Type": "String",
      "Default": "master"
    }
  }
}
